# agrotrust-az/.env.example

# App
VITE_APP_NAME=AgroTrust AZ

# Base API path (Netlify redirects will map this to functions in production)
VITE_API_BASE=/api

# Toggle mock data in the frontend
VITE_ENABLE_MOCKS=true

# Optional future auth settings
VITE_AUTH_PROVIDER=none
VITE_AUTH_DOMAIN=
VITE_AUTH_CLIENT_ID=
